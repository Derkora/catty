{
  "kind": "collectionType",
  "collectionName": "dokumens",
  "info": {
    "singularName": "dokumen",
    "pluralName": "dokumens",
    "displayName": "Dokumen",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "namaDokumen": {
      "type": "string"
    },
    "jenisDokumen": {
      "type": "enumeration",
      "enum": [
        "Dokumen_MataKuliah",
        "Dokumen_Administrasi"
      ]
    },
    "fileDokumen": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "extractedDokumen": {
      "type": "richtext"
    },
    "markdownFile": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "isProcessed": {
      "type": "boolean",
      "default": false
    }
  }
}
